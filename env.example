# RPCs
# Use either explicit RPC_URL(S) or set CHAIN to use presets.
#RPC_URL=https://your-primary-rpc
#RPC_URLS=https://rpc-1,https://rpc-2

# Chain preset (auto RPC if RPC_URL(S) empty)
# Supported presets: ethereum|eth, base, arbitrum|arb, optimism|op, polygon|matic, bnb|bsc
# For other chains like ape, hype, abstract â†’ please set RPC_URLS manually
#CHAIN=ethereum
#INFURA_KEY=
#ALCHEMY_KEY=

# Contract
CONTRACT_ADDRESS=0xYourContract
MINT_FUNC=mint
MINT_AMOUNT=1
#MINT_ARGS_JSON=["1"]
MINT_PRICE=0
#ABI_OVERRIDE=[{"inputs":[...],"name":"mint","outputs":[],"stateMutability":"payable","type":"function"}]

# Wallets
# Single
#PRIVATE_KEY=0x...
# Multi (comma separated)
#PRIVATE_KEYS=0x...,0x...,0x...

# Submission mode
SUBMIT_MODE=public # public|flashbots
#FLASHBOTS_RELAY=https://relay.flashbots.net
#FLASHBOTS_AUTH_KEY=0xYourAuthKey  # optional; random if omitted

# Pre-checks
CALLSTATIC_CHECK=true

# Fees
#GAS_PRICE_GWEI=20
#MAX_FEE_GWEI=100
#MAX_PRIORITY_GWEI=3
GAS_BUMP_PERCENT=15
RETRY_ATTEMPTS=5
RETRY_BACKOFF_MS=2000
RETRY_BACKOFF_MULTIPLIER=1.6

# Gas limit / estimation
#GAS_LIMIT=250000
GAS_LIMIT_BUFFER_PERCENT=20

# Nonce
NONCE_STRATEGY=pending # pending|latest|manual
#NONCE=0

# Multi-wallet execution
CONCURRENCY=4
TX_DELAY_MS=0
JITTER_MS=250

# Timing
WAIT_NEXT_BLOCK=false
# Start scheduling (one or both)
#START_AT_BLOCK=0
#START_AT_TIMESTAMP=0 # epoch ms or ISO string, e.g. 1717000000000 or 2025-01-01T00:00:00Z

# Safety
DRY_RUN=false

# Note: Untuk testnet, silakan isi RPC_URL(S) Anda sendiri dan (opsional) set CHAIN sesuai jika preset tersedia. Jika tidak, gunakan RPC_URLS manual. 